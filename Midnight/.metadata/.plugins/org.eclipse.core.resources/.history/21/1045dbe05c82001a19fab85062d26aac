package com.prac.json.run;

import java.io.File;
import java.io.FileReader;

import com.google.gson.Gson;
import com.google.gson.JsonObject;

public class Run {

	public static void main(String[] args) {

		Gson gson = new Gson();
		
		JsonObject jsonObject = gson.fromJson(new FileReader(new File("/resources/Book_top100.json")), JsonObject.class);
		
		
	}
}
