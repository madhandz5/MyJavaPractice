package com.kh.jdbc.practice;

public class Item {

	private String item;
	private String type;
	private int price;
	private int stock;

	public Item(String item, String type, int price, int stock) {
		super();
		this.item = item;
		this.type = type;
		this.price = price;
		this.stock = stock;
	}

	public String getItem() {
		return item;
	}

	public void setItem(String item) {
		this.item = item;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public int getStock() {
		return stock;
	}

	public void setStock(int stock) {
		this.stock = stock;
	}

}
