package bm.notice.model.dao;

import java.sql.Connection;
import java.sql.PreparedStatement;

import bm.notice.model.vo.Notice;
import common.JDBCTemplate;

public class NoticeDao {

	JDBCTemplate jdt = JDBCTemplate.getInstance();

	public int noticeUpload(Connection conn, Notice notice) {
		int res = 0;

		PreparedStatement pstm = null;
		String sql ="INSERT INTO TB_NOTICE VALUES(S_NOTICE.NEXTVAL,'제목입니다.',sysdate,'작성자','내용입니다.','오리지날파일경로','바뀐파일경로');";
		
		
		pstm = conn.prepareStatement(sql);		
		
		return res;

	}

}
